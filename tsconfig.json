{
	"compilerOptions": {
		"target": "es2017",
		"module": "none",
		"sourceMap": true,
		"outDir": "./dist",
		"moduleResolution": "node",
		"removeComments": true,
		"strict": true,
		"noImplicitAny": false,
		"strictNullChecks": false,
		"types": ["@overwolf/types"],
		"rootDirs": ["./src"]
	},
	"exclude": ["node_modules", "./src/in_game/", "./src/prompts/"]
}
